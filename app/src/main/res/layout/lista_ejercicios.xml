<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_ejercicios_salida_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="73dp"
        android:layout_marginTop="7dp"
        android:text="0"
        app:layout_constraintLeft_toRightOf="@+id/tv_lista_titulo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_ejercicios_salida_dia"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_lista_titulo"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="0"
        app:layout_constraintLeft_toLeftOf="@+id/tv_ejercicios_salida_nombre"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicios_salida_nombre" />

    <TextView
        android:id="@+id/tv_ejercicios_salida_semana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_lista_dia"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="0"
        app:layout_constraintLeft_toLeftOf="@+id/tv_ejercicios_salida_dia"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicios_salida_dia" />

    <TextView
        android:id="@+id/tv_ejercicios_salida_dificultad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="0"
        app:layout_constraintLeft_toLeftOf="@+id/tv_ejercicios_salida_semana"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicios_salida_semana" />

    <TextView
        android:id="@+id/tv_ejercicios_salida_serie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="0"
        app:layout_constraintLeft_toLeftOf="@+id/tv_ejercicios_salida_tiempo"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicios_salida_tiempo" />

    <TextView
        android:id="@+id/tv_ejercicios_salida_tiempo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="0"
        app:layout_constraintLeft_toLeftOf="@+id/tv_ejercicios_salida_dificultad"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicios_salida_dificultad" />

    <TextView
        android:id="@+id/tv_ejercicios_salida_repeticiones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-10dp"
        android:layout_marginTop="11dp"
        android:text="0"
        app:layout_constraintLeft_toLeftOf="@+id/tv_ejercicios_salida_serie"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicios_salida_serie" />

    <TextView
        android:id="@+id/tv_lista_titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="7dp"
        android:text="Nombre"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_lista_dia"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tv_lista_titulo"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="Dia"
