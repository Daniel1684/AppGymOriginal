        app:layout_constraintLeft_toLeftOf="@+id/tv_lista_semana"
        app:layout_constraintTop_toBottomOf="@+id/tv_lista_semana" />

    <TextView
        android:id="@+id/tv_lista_serie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="Serie"
        app:layout_constraintLeft_toLeftOf="@+id/tv_lista_tiempo"
        app:layout_constraintTop_toBottomOf="@+id/tv_lista_tiempo" />

    <TextView
        android:id="@+id/tv_lista_tiempo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        android:text="Tiempo"
        app:layout_constraintLeft_toLeftOf="@+id/tv_lista_dificultad"
        app:layout_constraintTop_toBottomOf="@+id/tv_lista_dificultad" />

    <TextView
        android:id="@+id/tv_lista_repeticiones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="7dp"
        android:text="Repeticiones"
        app:layout_constraintLeft_toLeftOf="@+id/tv_lista_serie"
        app:layout_constraintTop_toBottomOf="@+id/tv_lista_serie" />

    <TextView
        android:id="@+id/tv_lista_distancia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Distancia"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_lista_repeticiones"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/tv_lista_repeticiones" />

    <TextView
        android:id="@+id/tv_ejercicios_salida_distancia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="181dp" />

</android.support.constraint.ConstraintLayout>